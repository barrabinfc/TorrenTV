{
  "name": "TorrenTV",
  "version": "0.9.9",
  "homepage": "http://torrentv.github.io/" ,
  "description": "A torrent streaming client to VLC/AppleTV/XBMC via Airplay",
  "main": "app://host/src/app/index.html",
  "chromium-args": "--enable-threaded-compositing",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
      "type": "git",
      "url": "git@github.com:barrabinfc/TorrenTV.git"
  },
  "keywords": [
    "AppleTV",
    "Airplay",
    "ChromeCast",
    "Torrent"
  ],
  "author": {
      "name": "Carlos Guerrero"
  },
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "airplay-js": "*",
    "airplay-xbmc": "^0.2.10",
    "async": "^0.9.0",
    "chromecast-js": "git://github.com/yappco/chromecast-js.git#master",
    "combokeys": "^2.3.0",
    "connect": "^3.3.4",
    "connect-cors": "^0.5.6",
    "decompress": "^2.1.1",
    "font-awesome": "^4.3.0",
    "iconv-lite": "^0.4.7",
    "mime": "^1.3.4",
    "mousetrap": "^1.4.6",
    "mu2": "^0.5.20",
    "network-address": "1.0.0",
    "numeral": "^1.5.3",
    "nw-updater": "*",
    "peerflix": "*",
    "plist": "^1.1.0",
    "progressbar.js": "^0.7.4",
    "pump": "^1.0.0",
    "q": "^1.1.2",
    "range-parser": "^1.0.2",
    "read-torrent": "~1.2.0",
    "request": "^2.53.0",
    "rimraf": "^2.2.8",
    "semver": "^4.3.0",
    "srt2vtt2": "0.0.2",
    "subtitles-server": "0.0.3",
    "underscore": "^1.7.0",
    "video.js": "^4.11.4",
    "windows-no-runnable": "~0.0.6",
    "zepto": "0.0.4"
  },
  "window": {
    "title": "TorrenTV",
    "icon": "./src/app/media/images/icons/icon-app@2x.png",
    "frame": false,
    "resizable": false,
    "transparent": true,
    "show": false,
    "fullscreen": false,
    "toolbar": false,
    "always-on-top": false,
    "show-in-taskbar": true,
    "single-instance": true,
    "width": 350,
    "height": 300
  },
  "devDependencies": {
    "grunt": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-compress": "latest",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-exec": "latest",
    "grunt-jsvalidate": "latest",
    "grunt-node-webkit-builder": "latest",
    "load-grunt-tasks": "latest"
  }
}
