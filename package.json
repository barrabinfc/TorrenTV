{
  "name": "TorrenTV",
  "version": "0.9.9",
  "description": "A torrent streaming client to VLC/AppleTV/XBMC via Airplay",
  "main": "app://host/src/app/index.html",
  "chromium-args": "--enable-threaded-compositing",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "AppleTV",
    "Airplay",
    "Torrent"
  ],
  "author": "Carlos Guerrero",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.4.4",
    "airplay-js": "https://github.com/guerrerocarlos/node-airplay-js/tarball/master",
    "airplay-xbmc": "^0.2.1",
    "async": "^0.9.0",
    "chromecast-js": "*",
    "combokeys": "^2.0.0",
    "connect": "^3.0.2",
    "connect-cors": "^0.5.6",
    "decompress": "^2.0.0",
    "iconv-lite": "^0.4.5",
    "mime": "^1.2.11",
    "mousetrap": "^1.4.6",
    "mu2": "^0.5.20",
    "network-address": "0.0.4",
    "numeral": "^1.5.3",
    "nw-updater": "*",
    "peerflix": "0.10.3",
    "plist": "^1.1.0",
    "progressbar.js": "^0.6.1",
    "pump": "^1.0.0",
    "q": "^1.0.1",
    "range-parser": "^1.0.2",
    "read-torrent": "~1.0.0",
    "request": "^2.42.0",
    "rimraf": "^2.2.8",
    "semver": "^3.0.1",
    "srt2vtt2": "0.0.2",
    "underscore": "^1.7.0",
    "video.js": "^4.11.3",
    "windows-no-runnable": "~0.0.6",
    "zepto": "0.0.4"
  },
  "window": {
    "title": "TorrenTV",
    "icon": "./src/app/media/images/icons/icon.png",
    "frame": false,
    "resizable": false,
    "transparent": true,
    "show": false,
    "fullscreen": false,
    "toolbar": false,
    "always-on-top": false,
    "show-in-taskbar": true,
    "single-instance": true,
    "width": 350,
    "height": 300
  },
  "devDependencies": {
    "grunt": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-compress": "latest",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-exec": "latest",
    "grunt-jsvalidate": "latest",
    "grunt-node-webkit-builder": "latest",
    "load-grunt-tasks": "latest"
  }
}
